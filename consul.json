{
  "service": {
    "name": "${LANDINGS_SERVICE:-landings}",
    "tags": [
      "revision",
      "http",
      "http:${VSHKOLU_LANDING_SERVER_NAME:-vshkolu.shopfront.docker}",
      "http:${VODONAGREVATELI_LANDING_SERVER_NAME:-vodonagrevateli.shopfront.docker}",
      "http:${VELO_LANDING_SERVER_NAME:-velo.shopfront.docker}",
      "http:${GIFT_LANDING_SERVER_NAME:-gift.shopfront.docker}",
      "http:${IPHONE_LANDING_SERVER_NAME:-iphone.shopfront.docker}",
      "http:${FRANCHISE_LANDING_SERVER_NAME:-franchise.shopfront.docker}",
      "http:${CYBERMONDAY_LANDING_SERVER_NAME:-cybermonday.shopfront.docker}",
      "http:${BOSCH_DRILL_LANDING_SERVER_NAME:-bosch-drill.shopfront.docker}",
      "http:${BOILER_LANDING_SERVER_NAME:-boiler.shopfront.docker}",
      "http:${OPENBANK_LANDING_SERVER_NAME:-openbank.shopfront.docker}"
    ],
    "port": 80,
    "checks": [
      {
        "id": "landings",
        "service_id": "${LANDINGS_SERVICE:-landings}",
        "name": "http",
        "http": "http://localhost:80/",
        "interval": "10s",
        "timeout": "2s"
      }
    ]
  }
}
